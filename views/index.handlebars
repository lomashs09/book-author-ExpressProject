<h1>{{title}}</h1>

<table class="table mt-4 table table-striped">
    <thead>
        <tr>
            <th scope="col">Books</th>
            <th scope="col">Author ID</th>
            <th scope="col">Genre</th>
            <th scope="col">Book ID</th>
        </tr>
    </thead>
    <tbody>
        {{#each results}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.author_id}}</td>
            <td>{{this.GENRE}}</td>
            <td>{{this.book_id}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>